{
    "manifest_version": 2,
    "name": "PageFontstyle ",
    "version": "1.0",
    "description": "Change font of specific page",
    "browser_action": {
        "default_popup": "popup.html"
    },
    "background": {
        "scripts": [
            "eventPage.js"
        ],
        "persistent": false
    },
    "content_scripts":[
        {
            "matches":[""]
        }
    ],
    "permissions": [
        "tabs",
        "notifications",
        "contextMenus"
    ]
}